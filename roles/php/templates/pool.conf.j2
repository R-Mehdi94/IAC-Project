[{{ pool.name }}]
user = {{ pool.user }}
group = {{ pool.group }}

listen = {{ pool.listen }}

listen.owner = {{ pool.user }}
listen.group = {{ pool.group }}
listen.mode = 0660

pm = dynamic
pm.max_children = 5
pm.start_servers = 2
pm.min_spare_servers = 1
pm.max_spare_servers = 3

env[HOSTNAME] = $HOSTNAME
env[PATH] = /usr/local/bin:/usr/bin:/bin
env[TMP] = /tmp
